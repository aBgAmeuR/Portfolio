#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/Portfolio/node_modules/.pnpm/next-sitemap@4.2.3_next@15.1.4/node_modules/next-sitemap/bin/node_modules:/mnt/e/Portfolio/node_modules/.pnpm/next-sitemap@4.2.3_next@15.1.4/node_modules/next-sitemap/node_modules:/mnt/e/Portfolio/node_modules/.pnpm/next-sitemap@4.2.3_next@15.1.4/node_modules:/mnt/e/Portfolio/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/Portfolio/node_modules/.pnpm/next-sitemap@4.2.3_next@15.1.4/node_modules/next-sitemap/bin/node_modules:/mnt/e/Portfolio/node_modules/.pnpm/next-sitemap@4.2.3_next@15.1.4/node_modules/next-sitemap/node_modules:/mnt/e/Portfolio/node_modules/.pnpm/next-sitemap@4.2.3_next@15.1.4/node_modules:/mnt/e/Portfolio/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../next-sitemap/bin/next-sitemap.cjs" "$@"
else
  exec node  "$basedir/../next-sitemap/bin/next-sitemap.cjs" "$@"
fi
